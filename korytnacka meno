import turtle
t= turtle.Turtle()
t.pu()
t.setpos(-400,300)
t.pd()
#pismeno B
t.rt(90)
t.fd(180)
t.bk(190)
t.rt(220)
t.circle(-40,240)
t.rt(190)
t.circle(-90,160)
t.rt(90)
t.rt(90)
t.pu()
t.fd(120)
t.pd()
#pismeno E
t.fd(85)
t.bk(85)
t.lt(90)
t.fd(100)
t.rt(90)
t.fd(85)
t.bk(85)
t.lt(90)
t.fd(100)
t.rt(90)
t.fd(85)
t.pu()
t.fd(95)
t.pd()
t.rt(100)
